{
	"name": "lab-reservas",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"build:docker": "docker build -t lab-reservas . && docker network create lab-reservas-net || true",
		"dev": "remix vite:dev --host",
		"check": "biome check . && tsc",
		"start": "remix-serve ./build/server/index.js",
		"start:docker": "docker run --env-file .env.production --network lab-reservas-net -p 3000:3000 --name lab-reservas lab-reservas",
		"test": "vitest",
		"test:verbose": "vitest --reporter=verbose",
		"coverage": "vitest run --coverage",
		"kit": "drizzle-kit studio",
		"migration:generate": "drizzle-kit generate",
		"migration:push": "drizzle-kit push",
		"prepare": "husky"
	},
