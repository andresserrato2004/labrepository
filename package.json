{
	"name": "lab-reservas",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"build:docker": "docker build -t lab-reservas . && docker network create lab-reservas-net || true",
		"dev": "remix vite:dev --host",
		"check": "biome check . && tsc",
		"commit": "pnpm check && pnpm test run && git add . && git commit -am ",
		"start": "remix-serve ./build/server/index.js",
		"start:docker": "docker run --env-file .env.production --network lab-reservas-net -p 3000:3000 --name lab-reservas lab-reservas",
		"test": "vitest",
		"test:verbose": "vitest --reporter=verbose",
		"coverage": "vitest run --coverage",
		"kit": "drizzle-kit studio",
		"migration:generate": "drizzle-kit generate",
		"migration:push": "drizzle-kit push"
	},
	"dependencies": {
		"@nextui-org/button": "2.0.37",
		"@nextui-org/input": "2.2.4",
		"@nextui-org/link": "2.0.34",
		"@nextui-org/system": "2.2.5",
		"@nextui-org/theme": "2.2.9",
		"@paralleldrive/cuid2": "2.2.2",
		"@phosphor-icons/react": "2.1.7",
		"@remix-run/node": "2.11.1",
		"@remix-run/react": "2.11.1",
		"@remix-run/serve": "2.11.1",
		"argon2": "0.40.3",
		"classnames": "2.5.1",
		"cssnano": "7.0.5",
		"drizzle-orm": "0.33.0",
		"drizzle-zod": "0.5.1",
		"fast-jwt": "4.0.2",
		"framer-motion": "11.3.28",
		"isbot": "5.1.15",
		"jotai": "2.9.3",
		"postgres": "3.4.4",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"zod": "3.23.8",
		"zod-form-data": "2.0.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@remix-run/dev": "2.11.1",
		"@types/react": "18.3.3",
		"@types/react-dom": "18.3.0",
		"@typescript-eslint/eslint-plugin": "8.1.0",
		"@typescript-eslint/parser": "8.1.0",
		"autoprefixer": "10.4.20",
		"dotenv": "16.4.5",
		"drizzle-kit": "0.24.0",
		"postcss": "8.4.41",
		"tailwindcss": "3.4.10",
		"typescript": "5.5.4",
		"vite": "5.4.0",
		"vite-tsconfig-paths": "5.0.1",
		"vitest": "2.0.5"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
